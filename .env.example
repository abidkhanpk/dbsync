# Local PostgreSQL development database
DEV_DATABASE_URL="postgresql://pg_username:pg_password@localhost:5433/postgres?schema=public"

# Used by Prisma for safe schema diffing during migrate/dev operations
SHADOW_DATABASE_URL="postgresql://pg_username:pg_password@localhost:5433/_shadow_postgres?schema=public"

# Default DATABASE_URL points to dev for local work
DATABASE_URL="${DEV_DATABASE_URL}"
